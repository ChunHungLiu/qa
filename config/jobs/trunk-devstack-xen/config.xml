<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>This test polls nova trunk, and builds when people checkin to trunk.&#xd;
&lt;br&gt;&#xd;
It installs DevStack on two servers, and joins them together.&#xd;
&lt;br&gt;&#xd;
It then triggers devstack-xen-trunk-tests, if successful.</description>
  <logRotator>
    <daysToKeep>7</daysToKeep>
    <numToKeep>50</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.BooleanParameterDefinition>
          <name>CleanTemplates</name>
          <description>Clean the templates</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>DevStackURL</name>
          <description>&lt;h2&gt;The URL to the zip ball of your devstack repo&lt;/h2&gt;
&lt;ul&gt;
 &lt;li&gt;https://github.com/openstack-dev/devstack/zipball/master&lt;/li&gt;
 &lt;li&gt;https://github.com/citrix-openstack/devstack/zipball/xenserver-ci-improvements&lt;/li&gt;
 &lt;li&gt;https://github.com/citrix-openstack/devstack/zipball/fix-tgt-config&lt;/li&gt;
&lt;/ul&gt;</description>
          <defaultValue>https://github.com/citrix-openstack-build/devstack/zipball/master</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>PreseedURL</name>
          <description></description>
          <defaultValue>http://copper.eng.hq.xensource.com/devstackubuntupreseed.cfg</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>Server</name>
          <description></description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>localrcURL</name>
          <description></description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
    <com.sonyericsson.jenkins.plugins.externalresource.dispatcher.SelectionCriteria plugin="externalresource-dispatcher@1.0b">
      <selectionEnabled>false</selectionEnabled>
      <resourceSelectionList>
        <com.sonyericsson.jenkins.plugins.externalresource.dispatcher.selection.StringResourceSelection>
          <name>numberOfServers</name>
          <value>1</value>
        </com.sonyericsson.jenkins.plugins.externalresource.dispatcher.selection.StringResourceSelection>
      </resourceSelectionList>
    </com.sonyericsson.jenkins.plugins.externalresource.dispatcher.SelectionCriteria>
    <job-metadata plugin="metadata@1.0b">
      <values class="linked-list">
        <metadata-tree>
          <name>job-info</name>
